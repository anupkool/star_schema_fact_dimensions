    <sql>SELECT&#xd;&#xa;      &#x2f;&#x2a; fields that will be joined to dimensions in your surrogate key piplelining &#x2a;&#x2f;&#xd;&#xa;       p.printed_date, p.submitted_date, p.last_accessed_date, p.program_code,&#xd;&#xa;       p.applicant_client_id&#xd;&#xa;     , IFNULL&#x28;m.cnt_msg_app, 0&#x29; AS cnt_msg_app&#xd;&#xa;     , IFNULL&#x28;m.cnt_msg_sch, 0&#x29; AS cnt_msg_sch&#xd;&#xa;     , 1 AS is_applied&#xd;&#xa;     , CASE d.admission_decision WHEN &#x27;MATRICULATED&#x27; THEN 1&#xd;&#xa;                                 ELSE 0&#xd;&#xa;       END AS is_accepted&#xd;&#xa;	 , CASE d.admission_decision WHEN &#x27;ADMITTED&#x27; THEN 1 &#xd;&#xa;                                 WHEN &#x27;WITHRAW&#x27;  THEN 1&#xd;&#xa;								 WHEN &#x27;MATRICULATED&#x27; THEN 1  &#xd;&#xa;                                 ELSE 0 &#xd;&#xa;       END AS is_admitted,&#xd;&#xa;      SUBSTRING&#x28; MD5&#x28;CONCAT&#x28;&#xd;&#xa;                                     TRIM&#x28;IFNULL&#x28;ethnicity_indian,&#x27; &#x27;&#x29;&#x29;,&#xd;&#xa;                                     TRIM&#x28;IFNULL&#x28;ethnicity_asian,&#x27; &#x27;&#x29;&#x29;, &#xd;&#xa;                                     TRIM&#x28;IFNULL&#x28;ethnicity_black,&#x27; &#x27;&#x29;&#x29;,&#xd;&#xa;                                     TRIM&#x28;IFNULL&#x28;ethnicity_hawaiian, &#x27; &#x27;&#x29;&#x29;,&#xd;&#xa;                                     TRIM&#x28;IFNULL&#x28;ethnicity_white, &#x27; &#x27;&#x29;&#x29;,&#xd;&#xa;                                     TRIM&#x28;IF&#x28;hispanic_ethnicity &#x3d; &#x27;No&#x27;, &#x27; &#x27;, &#x27;Hispanic&#x27;&#x29;&#x29;&#xd;&#xa;                                    &#x29;&#xd;&#xa;                             &#x29;&#xd;&#xa;                          ,1,5&#xd;&#xa;                        &#x29; group_key&#xd;&#xa;     &#xd;&#xa;  FROM app_profile p&#xd;&#xa;  LEFT OUTER JOIN &#x28;&#xd;&#xa;        SELECT applicant_client_id, &#xd;&#xa;               SUM&#x28;IF&#x28;SENDER_TYPE&#x3d;&#x27;APP&#x27;, 1, 0&#x29;&#x29; AS cnt_msg_app,  &#xd;&#xa;               SUM&#x28;IF&#x28;SENDER_TYPE&#x3d;&#x27;SCH&#x27;, 1, 0&#x29;&#x29; AS cnt_msg_sch &#xd;&#xa;          FROM adm_messages&#xd;&#xa;         GROUP BY applicant_client_id&#xd;&#xa;       &#x29; m&#xd;&#xa;    ON p.applicant_client_id &#x3d; m.applicant_client_id&#xd;&#xa;  LEFT OUTER JOIN adm_decision d&#xd;&#xa;    ON p.applicant_client_id &#x3d; d.applicant_client_id&#xd;&#xa;&#x3b;&#xd;&#xa;</sql>
